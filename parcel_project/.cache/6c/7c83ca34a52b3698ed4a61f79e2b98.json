ource":"index.js","original":{"line":81,"column":1}},{"generated":{"line":108,"column":1},"source":"index.js","original":{"line":79,"column":0}},{"generated":{"line":110,"column":0},"source":"index.js","original":{"line":83,"column":0},"name":"divDecrease"},{"generated":{"line":110,"column":11},"source":"index.js","original":{"line":83,"column":11}},{"generated":{"line":110,"column":12},"source":"index.js","original":{"line":83,"column":12},"name":"onClick"},{"generated":{"line":110,"column":19},"source":"index.js","original":{"line":83,"column":0}},{"generated":{"line":110,"column":22},"source":"index.js","original":{"line":83,"column":22}},{"generated":{"line":110,"column":34},"source":"index.js","original":{"line":83,"column":28}},{"generated":{"line":111,"column":2},"source":"index.js","original":{"line":84,"column":4},"name":"store"},{"generated":{"line":111,"column":7},"source":"index.js","original":{"line":84,"column":9}},{"generated":{"line":111,"column":8},"source":"index.js","original":{"line":84,"column":10},"name":"dispatch"},{"generated":{"line":111,"column":16},"source":"index.js","original":{"line":84,"column":4}},{"generated":{"line":111,"column":17},"source":"index.js","original":{"line":84,"column":19},"name":"decrease"},{"generated":{"line":111,"column":25},"source":"index.js","original":{"line":84,"column":27}},{"generated":{"line":111,"column":27},"source":"index.js","original":{"line":84,"column":4}},{"generated":{"line":112,"column":0},"source":"index.js","original":{"line":85,"column":1}},{"generated":{"line":112,"column":1},"source":"index.js","original":{"line":83,"column":0}}],"sources":{"index.js":"import { legacy_createStore } from 'redux'; //그냥 createStore 하면 선그어지네 \n\n//react를 사용하지 않기 때문에 DOM을 직접 수정해줘야 한다. \n//다음과 같이 js 상단에 수정할 DOM 노르를 가리키는 값을 미리 선언해 준다. \nconst divToggle = document.querySelector('.toggle'); //html의 id 값이 toggle인 것을 불러와서 정의 하였다. \nconst counter = document.querySelector('h1');\nconst btnIncrease = document.querySelector('#increase');\nconst btnDecrease = document.querySelector('#decrease');\n\n//액션 이름 정의하기 \nconst TOGGLE_SWITCH = 'TOGGLE_SWITCH';\nconst INCREASE = 'INCREASE';\nconst DECREASE = 'DECREASE';\n\n//액션 객체를 만드는 액션 생성 함수 작성하기\nconst toggleSwitch = () => ({ type: TOGGLE_SWITCH }); //type 안에 들어갈 값은 액션의 이름이다. \nconst increase = difference => ({type: INCREASE, difference});\nconst decrease = () => ({type: DECREASE});\n\n//초깃값 설정\nconst initialState = {\n    toggle: false,\n    counter: 0\n};\n\n//리듀서함수 정의 \nfunction reducer(state=initialState, action) {\n    //action.type 값에 따라 다른 작업을 처리함\n    switch (action.type) {\n        case TOGGLE_SWITCH:\n            return {\n                ...state, //불변성 유지를 해 주어야 한다. \n                toggle: !state.toggle //toggle 만 true면 false로 false면 true로 바꿔준다. \n            };\n        case INCREASE: \n            return {\n                ...state,\n                counter: state.counter + action.difference //difference로 나타냈지 그냥 +1 하면 안되나\n            };\n        case DECREASE:\n            return {\n                ...state,\n                counter: state.counter - 1\n            };\n        default:\n            return state;    \n        \n    }\n}\n\nconst store = legacy_createStore(reducer);\n\n//상태가 업데이트될 때마다 호출한다. \nconst render = () => {\n    const state = store.getState() //현재 상태를 불러온다. 이런게 어렵다. \n    //토글 처리\n    if (state.toggle) {\n        divToggle.classList.add('active');\n    } else {\n        divToggle.classList.remove('active');\n    }\n    counter.innerText = state.counter;\n\n}\n\n//위에는 render 할 내용을 정의만 해준 상태 \n//subscribe\n//store의 상태가 바뀔때마다 위에서 만든 render 함수가 호출되도록 해준다. \n\nrender();\nstore.subscribe(render);\n\n//액션을 발생시킨다. = dispatch\n//파라미터에는 액션 객체를 넣어준다. \ndivToggle.onclick = () => {\n    store.dispatch(toggleSwitch());\n}; //toggle에 클릭 이벤트가 일어나면 toggleSwitch 액션을 발생 시킨다. \n\ndivIncrease.onclick = () => {\n    store.dispatch(increase());\n};\n\ndivDecrease.onClick = () => {\n    store.dispatch(decrease());\n};\n\n\n\n"},"lineCount":null}},"error":null,"hash":"e7f9f71d83562586a31a548c2413b670","cacheData":{"env":{}}}